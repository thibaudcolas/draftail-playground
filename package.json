{
  "name": "draftail-playground",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not IE 11"
    ],
    "development": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not IE 11"
    ]
  },
  "devDependencies": {
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "enzyme-to-json": "3.6.2",
    "prettier": "2.3.0",
    "react-scripts": "4.0.3",
    "react-test-renderer": "16.14.0",
    "source-map-explorer": "2.4.1",
    "typescript": "4.2.4"
  },
  "dependencies": {
    "@types/draft-js": "^0.10.29",
    "@types/jest": "26.0.23",
    "@types/node": "14.17.0",
    "@types/prismjs": "^1.16.0",
    "@types/react": "16.14.6",
    "@types/react-dom": "16.9.13",
    "@types/react-modal": "^3.8.1",
    "@types/react-resizable": "^1.7.0",
    "@types/react-tabs": "^2.3.1",
    "@types/styled-components": "5.1.9",
    "draft-js": "^0.10.5",
    "draftail": "^1.3.0",
    "prismjs": "^1.6.0",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-json-view": "^1.19.1",
    "react-modal": "^3.8.1",
    "react-resizable": "^1.7.5",
    "react-simpler-select": "^3.0.0",
    "react-tabs": "^3.0.0",
    "reading-time": "^1.1.0",
    "styled-components": "5.3.0"
  },
  "scripts": {
    "start": "npm run server -s & npm run watch -s",
    "server": "echo 'start separately'",
    "watch": "react-scripts start",
    "build": "CI=true react-scripts build && source-map-explorer --html build/static/js/main.* > build/source-map-explorer.html",
    "lint": "prettier --check '**/?(.)*.{md,css,scss,js,jsx,ts,tsx,json,yaml,yml,html}' && black --check .",
    "format": "prettier --write '**/?(.)*.{md,css,scss,js,jsx,ts,tsx,json,yaml,yml,html}' && black .",
    "report:build": "open public/source-map-explorer.html",
    "test": "CI=true react-scripts test",
    "test:watch": "react-scripts test --watch",
    "test:coverage": "CI=true react-scripts test --coverage",
    "test:ci": "npm run lint -s && npm run test:coverage && npm run build -s"
  }
}
